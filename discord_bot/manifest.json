
{
  "display_information": {
    "name": "Roblox Grup Yönetimi",
    "description": "Roblox grupları için kapsamlı yönetim botu",
    "background_color": "#0099ff"
  },
  "features": {
    "bot_user": {
      "display_name": "Roblox Grup Bot",
      "always_online": true
    }
  },
  "oauth_config": {
