      "bot"
    ],
    "bot_permissions": {
      "send_messages": true,
      "embed_links": true,
      "read_message_history": true,
      "use_application_commands": true
    }
  },
  "settings": {
    "event_subscriptions": {
      "request_url": "",
      "bot_events": [
        "message.channels"
      ]
    }
  }
}
